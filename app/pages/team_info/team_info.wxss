/* pages/team_info/team_info.wxss */

.team-info-head {
  padding: 100rpx 30rpx;
  box-sizing: border-box;
  color: #fff;
  font-size: 28rpx;
  height: 300rpx;
  background: url('https://wapapi.it919.cn/static/images/bg.png') center
    no-repeat;
  background-size: cover;
}

.head-info {
  width: 40%;
}

.team-logo image {
  width: 120rpx;
  height: 120rpx;
  margin: 0 auto;
  display: block;
  text-align: center;
}

.head-info text {
  display: block;
}

.team-logo {
  width: 20%;
}

.swiper-tab {
  width: 100%;
  border-bottom: 1rpx solid #bebebe;
  text-align: center;
  line-height: 80rpx;
}

.swiper-tab-list {
  font-size: 30rpx;
  display: inline-block;
  width: 33.333%;
  color: #777;
}

.on {
  color: #00aa98;
}

.swiper-box {
  display: block;
  width: 100%;
  box-sizing: border-box;
  height: auto;
  min-height: 100%;
  float: left;
}

.swiper-item {
  width: 100%;
  height: 100%;
  display: none;
  box-sizing: border-box;
}

view.active {
  display: block;
}

.teamHonor {
  width: 100%;
  padding: 20rpx 0;
  background: #f5f5f5;
  float: left;
  color: #333;
}

.honor-item {
  width: 33.33%;
  float: left;
  font-size: 26rpx;
  text-align: center;
}

.honor-item .rank {
  background: url(https://wapapi.it919.cn/static/images/ranks.png) center
    no-repeat;
}

.honor-item .no1 {
  background: url(https://wapapi.it919.cn/static/images/no1.png) center
    no-repeat;
}

.honor-item .master {
  background: url(https://wapapi.it919.cn/static/images/master.png) center
    no-repeat;
}

.honor-item .icon {
  padding: 30rpx;
  background-size: 45rpx;
}

.skin-statsTech {
  margin-top: 30rpx;
  width: 100%;
  height: 200rpx;
  float: left;
  border-top: 1rpx solid #bebebe;
}

.skin-statsTech .t-head {
  background: #f5f5f5;
  height: 70rpx;
  line-height: 70rpx;
  border-top: 1rpx solid #ddd;
  border-bottom: 1rpx solid #ddd
}

.skin-statsTech .t-head text,
.skin-statsTech .t-body text {
  font-size: 28rpx;
  display: block;
  width: 20%;
  text-align: center;
  float: left;
  box-shadow: inset 0px -1rpx 1px -1rpx #bebebe;
}

.skin-statsTech .t-body {
  font-size: 28rpx;
}

.t-body text {
  height: 70rpx;
  line-height: 70rpx;
}

.data-rank {
  width: 100%;
  margin-top: 30rpx;
  float: left;
  background: #fff;
}

.data-rank-title {
  height: 90rpx;
  background: #f6f6f6;
  font-size: 28rpx;
  width: 100%;
  line-height: 90rpx;
  float: left;
  box-sizing: border-box;
  padding: 0 30rpx;
  border-top: 1rpx solid #bebebe;
}

.data-rank-item {
  font-size: 28rpx;
  width: 100%;
  box-sizing: border-box;
  padding: 0 30rpx;
  line-height: 70rpx;
  border-top: 1rpx solid #bebebe;
  float: left;
}

.list-player {
  width: 100%;
  box-sizing: border-box;
  padding: 10rpx 20rpx;
  background: #f2f7fc;
  float: left;
}

.item-player {
  width: 33.33%;
  float: left;
  font-size: 28rpx;
  overflow: hidden;
  border: 3px solid #f2f7fc;
  box-sizing: border-box;
  background: #fff;
  padding: 10rpx;
}

.item-player image {
  width: 220rpx;
  height: 180rpx;
  display: block;
}

.item-player text {
  display: block;
  font-size: 24rpx;
  padding: 1px 10px;
}

.item-player .pname {
  font-size: 28rpx;
  width: 100%;
  height: 50rpx;
  overflow: hidden;
  line-height: 50rpx;
}

.select-mouth {
  padding: 30rpx;
  font-size: 28rpx;
  border-bottom: 1rpx solid #bebebe;
}

/***************************赛程*******************  */

.live_item {
  min-height: 160rpx;
  height: auto;
  overflow: hidde;
  background: #fff;
  box-sizing: border-box;
  padding: 30rpx 10px;
  margin-top: 10px;
  box-shadow: inset 0px -1px 1px -1px #bebebe;
}

.team {
  width: 40%;
  height: 100rpx;
  float: left;
  text-align: center;
}

.team-logo {
  width: 100rpx;
  height: 100rpx;
  float: left;
}

.team-info {
  text-align: center;
  float: left;
  width: 180rpx;
}

.team-info view {
  display: block;
}

.team-score {
  font-size: 40rpx;
}

.team-name {
  font-size: 28rpx;
}

.time {
  height: 100rpx;
  float: left;
  width: 20%;
  text-align: center;
  font-size: 32rpx;
  line-height: 50rpx;
}

.team-news {
  font-size: 28rpx;
  width: 100%;
}

.iangle {
  color: #fff;
  padding: 0px 3px;
  font-size: 22rpx;
  background: #00aa98;
}

.icon-rili {
  background: url('data:image/png;base64,')
    280rpx no-repeat;
  background-size: 40rpx;
}
