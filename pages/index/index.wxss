/* pages/nba/nba.wxss */

.date-list {
    width: 100%;
    background: #f2f7fc;
    box-sizing: border-box;
    padding: 0 20rpx;
    height: 100rpx;
    line-height: 100rpx;
}

.date-item {
    width: 33.33%;
    float: left;
    font-size: 30rpx;
    text-align: center;
    height: 100rpx;
    color: #00aa98
}

.live_item {
    min-height: 160rpx;
    height: auto;
    overflow: hidde;
    background: #fff;
    box-sizing: border-box;
    padding: 30rpx 10px;
    margin-top: 10px;
    box-shadow: inset 0px -1px 1px -1px #999;
}

.team {
    width: 40%;
    height: 100rpx;
    float: left;
    text-align: center;
}

.team-logo {
    width: 100rpx;
    height: 100rpx;
    float: left;
}

.team-info {
    text-align: center;
    float: left;
    width: 180rpx;
}

.team-info view {
    display: block;
}

.team-score {
    font-size: 40rpx;
}

.team-name {
    font-size: 28rpx;
}

.time {
    height: 100rpx;
    float: left;
    width: 20%;
    text-align: center;
    font-size: 32rpx;
    line-height: 50rpx;
}

.team-news {
    font-size: 28rpx;
    width: 100%;
}

.iangle {
    color: #fff;
    padding: 0px 3px;
    font-size: 22rpx;
    background: #00aa98;
}

page {
    position: relative;
}

.refesh {
    width: 84rpx;
    height: 84rpx;
    background: rgba(0, 170, 152, 0.6) url("data:image/png;base64,") center no-repeat;
    background-size: 50rpx;
    border-radius: 50%;
    position: fixed;
    right: 10px;
    bottom: 110rpx;
    font-size: 28rpx;
    color: #fff;
    -webkit-transition: 0.4s;
    -webkit-transition: -webkit-transform 0.4s ease-out;
    transition: transform 0.4s ease-out;
    -moz-transition: -moz-transform 0.4s ease-out;
}

.refesh:active {
    transform: rotateZ(720deg);
    -webkit-transform: rotateZ(720deg);
    -moz-transform: rotateZ(720deg);
}

.icon-pre {
    background: url("data:image/png;base64,") center left 90rpx no-repeat;
    background-size: 60rpx;
}

.icon-next {
    background: url("data:image/png;base64,") center right 90rpx no-repeat;
    background-size: 60rpx;
}

.icon-picker {
    background: url("data:image/png;base64,") right no-repeat;
    background-size: 40rpx;
    padding: 50rpx 25rpx;
}